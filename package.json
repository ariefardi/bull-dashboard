{
  "name": "bull-board",
  "description": "Bull queue UI for inspecting jobs",
  "keywords": [
    "bull",
    "redis",
    "queue"
  ],
  "main": "index.js",
  "private": false,
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vcapretz/bull-board.git"
  },
  "bugs": {
    "url": "https://github.com/vcapretz/bull-board/issues"
  },
  "scripts": {
    "prepublishOnly": "rm -rf ./build && yarn build",
    "test": "node ./dockest.js",
    "build": "NODE_ENV=production webpack",
    "build:watch": "NODE_ENV=production webpack --watch",
    "start": "nodemon redis.js",
    "start:dev": "concurrently \"yarn build:watch\" \"yarn start\"",
    "start:redis": "yarn build && yarn start"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.17.2",
    "bull": "3.11.0",
    "date-fns": "2.4.1",
    "ejs": "^2.6.1",
    "express": "^4.15.2",
    "express-async-router": "^0.1.15",
    "pretty-bytes": "^5.1.0",
    "ramda": "^0.26.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-highlight": "^0.12.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "babel-preset-react-app": "^7.0.2",
    "concurrently": "^4.1.2",
    "css-loader": "^2.1.1",
    "dockest": "^1.0.3",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react-dev-utils": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
